<%= form_for :participant, url: participant_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :photo_session_id %><br>
    <%= f.text_field :photo_session_id %>
  </p>

  <p>
    <%= f.submit "add person" %>
  </p>
<% end %>
